    "class-validator": "^0.14.1",
    "cloudinary": "^2.2.0",
    "colors": "^1.4.0",
    "dataloader": "^2.2.2",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.0",
    "graphql": "^16.8.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0",
